(define (problem dock)
    (:domain dockDomain)
    (:objects d1 d2 d3 c1 c2 c3 r1 )
    (:INIT (truck r1) 
            (pier d1) (pier d2) (pier d3) 
            (container c1) (container c2) (container c3)
            (connected d1 d3) (connected d3 d1)
            (connected d1 d2) (connected d2 d1)
            (at r1 d2) (free r1) 
            (not (empty d2)) (empty d1) (empty d3)
            (located c1 d1) (located c2 d2) (located c3 d1)
            (not (clear c1)) (clear c2) (clear c3)
            (on c3 c1) (onPier c2) (onPier c1)
    )
    (:goal (AND (at r1 d1) (located c1 d3) (located c2 d3) (located c3 d3) 
                (on c1 c2))
    )
)
